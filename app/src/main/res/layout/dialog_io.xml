<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="wrap_content" android:layout_height="wrap_content"
	android:paddingLeft="2sp" android:orientation="vertical">
	<LinearLayout android:orientation="horizontal"
		android:layout_width="fill_parent" android:layout_height="wrap_content"
		android:gravity="center_vertical">
		<Button android:text="@string/BROWSE" android:id="@+id/dialog_io_browse"
			android:layout_width="wrap_content" android:layout_height="wrap_content" />
		<TextView android:id="@+id/dialog_io_filename" android:text="cat"
			android:gravity="center_vertical" android:layout_width="wrap_content"
			android:layout_height="wrap_content" />
	</LinearLayout>
	<CheckBox android:id="@+id/dialog_io_overwrite" android:text="@string/OVERWRITE"
		android:layout_width="fill_parent" android:layout_height="wrap_content" />
</LinearLayout>
